#include <stdio.h>

struct person {
  char name[50];
  int age;
  float height;
};

int main() {
  struct person p;


  printf("Enter name: ");
  scanf("%s", p.name);

  printf("Enter age: ");
  scanf("%d", &p.age);

  printf("Enter height (in meters): ");
  scanf("%f", &p.height);


  printf("Name: %s\n", p.name);
  printf("Age: %d\n", p.age);
  printf("Height: %.2f meters\n", p.height);

  return 0;
}
