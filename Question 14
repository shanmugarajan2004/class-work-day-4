#include <stdio.h>


struct rectangle {
  int length;
  int width;
};


int calculate_area(struct rectangle r) {
  int area = r.length * r.width;
  return area;
}

int main() {
  
  struct rectangle my_rectangle = {5, 10};

 
  int area = calculate_area(my_rectangle);
  printf("The area of the rectangle is %d.\n", area);

  return 0;
}
